      <geometry>
        <cylinder length="0.025" radius="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>
  <gazebo reference="rear_right_wheel_steer">
    <mu1>0.8</mu1>
    <mu2>0.8</mu2>
    <material>Gazebo/GreyTransparent</material>
  </gazebo>
  <joint name="rear_right_wheel_steer_joint" type="continuous">
    <origin rpy="0 0 0" xyz="0.0 0.0 -0.1"/>
    <parent link="rear_right_wheel_suspension"/>
    <child link="rear_right_wheel_steer"/>
    <axis xyz="0 0 1"/>
  </joint>
  <transmission name="rear_right_wheel_steer_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="rear_right_wheel_steer_joint">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="rear_right_wheel_steer_motor">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
  <link name="rear_right_wheel_rotor">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.2"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.2"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="5.0"/>
      <inertia ixx="0.0029167" ixy="0" ixz="0" iyy="0.0029167" iyz="0" izz="0.0056250"/>
    </inertial>
  </link>
  <gazebo reference="rear_right_wheel_rotor">
    <mu1>0.8</mu1>
    <mu2>0.8</mu2>
    <material>Gazebo/DarkGrey</material>
  </gazebo>
  <joint name="rear_right_wheel_rotor_joint" type="continuous">
    <origin rpy="-1.57 0 0" xyz="0 0 -0.225"/>
    <parent link="rear_right_wheel_steer"/>
    <child link="rear_right_wheel_rotor"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.1"/>
  </joint>
  <transmission name="rear_right_wheel_rotor_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="rear_right_wheel_rotor_joint">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="rear_right_wheel_rotor_motor">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
  <!-- # gazebo settings -->
  <!-- ## import plugin "gazebo_ros_control" -->
  <gazebo>
    <plugin filename="libgazebo_ros_control.so" name="gazebo_ros_control">
      <robotNamespace>/fwids</robotNamespace>
    </plugin>
  </gazebo>
  <!-- ## activate laser -->
  <gazebo reference="laser_link">
    <material>Gazebo/Blue</material>
    <sensor name="laser" type="ray">
      <pose>0 0 0 0 0 0</pose>
      <visualize>false</visualize>
      <update_rate>10</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>800</samples>
            <resolution>1</resolution>
            <min_angle>-3.141592653589793</min_angle>
            <max_angle>3.141592653589793</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.05</min>
          <max>30.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin filename="libgazebo_ros_laser.so" name="gazebo_ros_lidar_controller">
        <topicName>/laser_link/scan</topicName>
        <frameName>laser_link</frameName>
      </plugin>
    </sensor>
  </gazebo>
  <!-- ## publish true odometry value -->
  <gazebo>
    <plugin filename="libgazebo_ros_p3d.so" name="groundtruth">
      <alwaysOn>true</alwaysOn>
      <updateRate>50.0</updateRate>
      <bodyName>base_link</bodyName>
      <topicName>groundtruth_odom</topicName>
      <frameName>map</frameName>
      <xyzOffsets>0 0 0</xyzOffsets>
      <rpyOffsets>0 0 0</rpyOffsets>
      <gaussianNoise>0.02</gaussianNoise>
    </plugin>
  </gazebo>
</robot>

